@Article{gans94:mighty,
  title =	 {How are the mighty fallen: Rejected classic articles
                  by leading economists},
  author =	 {Gans, Joshua S and Shepherd, George B},
  journal =	 {Journal of Economic Perspectives},
  volume =	 8,
